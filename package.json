{
  "name": "@op-engineering/op-ocr",
  "version": "1.0.0",
  "description": "VisionCamera Frame Processor Plugin to detect and read MRZ data from passports using MLKit Text Recognition.",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*",
    "!docs/**"
  ],
  "scripts": {
    "example": "yarn workspace VisionCameraMrzScannerExample",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build",
    "prepare": "bob build",
    "typecheck": "tsc"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "vision-camera",
    "mrz",
    "MLKit",
    "passport",
    "vision",
    "camera",
    "scanner",
    "ocr"
  ],
  "repository": "https://github.com/OP-Engineering/op-ocr",
  "author": "Oscar Franco (ospfranco@gmail.com> (https://github.com/ospfranco)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OP-Engineering/op-ocr/issues"
  },
  "homepage": "https://github.com/OP-Engineering/op-ocr#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "mrz": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native-community/cli": "20.0.1",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "^0.81.1",
    "@types/react": "^19.1.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^2.8.8",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.15",
    "react-native-reanimated": "~4.1.5",
    "react-native-vision-camera": "^4.7.1",
    "react-native-worklets": "^0.6.1",
    "react-native-worklets-core": "^1.6.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": ">= 4",
    "react-native-vision-camera": ">= 4"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
